@model WebApp.Areas.Admin.ViewModels.PaymentCreateEditVm


<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<div class="form-group">
    <label asp-for="Payment.PaymentDate" class="control-label"></label>
    <input asp-for="Payment.PaymentDate" class="form-control"/>
    <span asp-validation-for="Payment.PaymentDate" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Payment.PaymentValue" class="control-label"></label>
    <input asp-for="Payment.PaymentValue" class="form-control"/>
    <span asp-validation-for="Payment.PaymentValue" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Payment.OwnerId" class="control-label"></label>
    <select asp-for="Payment.OwnerId" class="form-control" asp-items="Model!.OwnerSelectList"></select>
</div>
<div class="form-group">
    <label asp-for="Payment.FundId" class="control-label"></label>
    <select asp-for="Payment.FundId" class="form-control" asp-items="Model.FundSelectList"></select>
</div>
<div class="form-group">
    <label asp-for="Payment.PersonId" class="control-label"></label>
    <select asp-for="Payment.PersonId" class="form-control" asp-items="Model.PersonSelectList"></select>
</div>
